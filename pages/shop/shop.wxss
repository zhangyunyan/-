/* pages/shop/shop.wxss */

.container {
  display: flex;
  flex-direction: column;
  width: 750rpx;
}

.title {
  font-size: 20rpx;
  text-align: center;
  line-height: 40rpx;
  margin-top: 30rpx;
}

.title view {
  width: 174rpx;
  height: 0;
  border-bottom: 1px solid red;
}

.title .big {
  font-size: 44rpx;
  line-height: 64rpx;
}

.types {
  display: flex;
  flex-direction: row;
  width: 710rpx;
  justify-content: space-around;
  margin-top:30rpx;
}

.types .typesItem {
  width: 218rpx;
  /* width: 33%; */
  text-align: center;
  border: 1px solid #eee;
}

.types .typesItem .bgBorder {
  display: flex;
  flex-direction: column;
  margin: 4rpx;
  height: 218rpx;
  border: 1px solid #eee;
  justify-content: center;
  font-size: 30rpx;
}

.types .typesItem .bgBorder view {
  margin: 0 auto;
  width: 150rpx;
  height: 0;
  border-bottom: 1px dotted red;
}

.list{
  display: flex;
  flex-direction: row;
   flex-wrap: wrap; 
   justify-content: space-around; 
  width: 710rpx;
}
.list .out{
  display: flex;
  flex-direction: column;
  width: 340rpx;
  height: 408rpx;
  background-color:#eee;
  margin-bottom: 10rpx;
}
.list .out image{
  margin: 10rpx;
  width: 320rpx;
  height: 320rpx;
}
.list .out view{
  margin: 0 auto;
  width: 271rpx;
  height: 45rpx;
  font-size: 24rpx;
  background: url(data:image/png;base64,) no-repeat;
background-size: 271rpx 45rpx;
}
.list .out view .buy{
  /* display: inline-block; */
  margin-left: 66rpx;
  color: white;
}